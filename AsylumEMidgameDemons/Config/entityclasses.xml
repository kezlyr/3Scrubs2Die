<!--   Belphegor - Sloth Demon Lord   -->
<entity_class name="belphegor" extends="zombieTemplateMale">
<property name="Tags" value="entity,zombie,endgame,belphegor"/>
<property name="Mesh" value="#@modfolder:Resources/EndgameZombies.unity3d?bigdemon"/>
<property name="PhysicsBody" value="endgameZombieBody"/>
