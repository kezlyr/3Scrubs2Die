<property name="MoveSpeedPattern" value="^infernalMoveSpeedPattern"/>
<property name="WalkType" value="2"/>
<property name="Immunity" value="sickness;disease"/>
<property name="DeadBodyHitPoints" value="1000"/>
<property name="LootDropEntityClass" value="EntityLootContainerBoss"/>
<property name="LootDropProb" value="0.4"/>
