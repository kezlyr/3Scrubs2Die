<configs>
	<!-- Infernal Enhancement System - Recipes -->

	<!-- Black Stone Crafting (from Infernal materials) -->
	<append xpath="/recipes">
		<recipe name="infernalBlackStone" count="5" craft_area="chemistryStation" craft_time="60" tags="chemStationCrafting">
			<ingredient name="resourceCoal" count="10"/>
			<ingredient name="resourceOil" count="5"/>
			<ingredient name="resourceAcid" count="3"/>
		</recipe>
	</append>
	
	<!-- Concentrated Stone Crafting -->
	<append xpath="/recipes">
		<recipe name="infernalConcentratedStone" count="1" craft_area="chemistryStation" craft_time="120" tags="chemStationCrafting">
			<ingredient name="infernalBlackStone" count="10"/>
			<ingredient name="resourceAcid" count="5"/>
			<ingredient name="resourceOil" count="10"/>
		</recipe>
	</append>
	
	<!-- Cron Stone cannot be crafted - loot only -->

