<buff name="BuffHosting" icon="ui_game_symbol_agility" icon_color="132,0,155" hidden="true"> <!-- Small Flood -->
		<stack_type value="replace"/>
		<duration value="300"/> <!-- 5 mins, this also functions as a cooldown from "resetting" the debuff while it's already active -->
		<update_rate value="1"/>
		<effect_group>
			<triggered_effect trigger="onSelfBuffStart" action="CallGameEvent" event="action_Flood_Burrow" /> <!-- if using this in your own mods be very careful, triggering gameevents can be sus if you don't know what you're doing -->
		</effect_group>
	</buff>

<buff name="Hosting" name_key="Hosting" icon="ui_game_symbol_zombie" icon_color="255,0,0" description_key="THE FLOOD HAVE BURROWED INTO YOUR SYSTEM. ALL SKILLS AND PERKS ARE REDUCED TO 1." hidden="false">
		<stack_type value="replace"/>
		<duration value="300"/>
		<update_rate value="1"/>

		<effect_group> <!-- Take away the players perks/skills for a short time, min maxers in absolute shambles tbh -->
			<triggered_effect trigger="onSelfBuffStart" action="RemoveBuff" buff="buffme"/> <!-- this is a console command "buff buffme" which overrides the debuff, this cancels that out if you're using it for whatever reason (looking at you cheeky admins) -->
			<passive_effect name="AttributeLevel" operation="base_set" value="1"/>
			<triggered_effect trigger="onSelfBuffStart" action="RefreshPerks"/>
			<triggered_effect trigger="onSelfBuffRemove" action="RefreshPerks"/>

			<passive_effect name="SkillLevel" operation="base_set" value="1"/>
			<passive_effect name="PerkLevel" operation="base_set" value="0"/>
		</effect_group>
	</buff>
	
