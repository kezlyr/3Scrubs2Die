<configs>

	<append xpath="/items">
		
	<item name="SirenHand">
		<property name="Extends" value="meleeHandMaster"/>
		<property name="CreativeMode" value="None"/>
		<property class="Action0">
			<property name="Range" value="2.3"/> 
			<property name="DamageEntity" value="8"/>
			<property name="DamageBlock" value="22"/>
			<property name="Buff" value="Cursed"/>
		</property>
		<effect_group name="SirenHand" tiered="false">
			<passive_effect name="ModSlots" operation="base_set" value="0"/>
			<passive_effect name="DamageModifier" operation="perc_add" value="3" tags="earth"/>
