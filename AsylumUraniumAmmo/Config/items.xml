<!-- *** INCENDIARY AMMUNITION *** -->

<!-- 9mm Incendiary Bullet -->
<append xpath="/items">
<item name="ammoIncendiaryBullet9mm">
	<property name="Tags" value="ammo,ammo9mm"/>
	<property name="DisplayType" value="ammoBullet"/>
	<property name="DescriptionKey" value="ammoIncendiaryBullet9mmDesc"/>
	<property name="CustomIcon" value="ammo9mmBulletBall"/>
	<property name="CustomIconTint" value="FF6600"/>
	<property name="HoldType" value="45"/>
	<property name="Meshfile" value="@:Other/Items/Misc/sackPrefab.prefab"/>
	<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
	<property name="Material" value="Mbrass"/>
	<property name="MeltTimePerUnit" value=".4"/>
	<property name="Stacknumber" value="150"/>
	<property name="EconomicValue" value="40"/>
	<property name="TraderStageTemplate" value="ammoTier3"/>
	<property name="Group" value="Ammo/Weapons,Ammo,Ranged Weapons"/>
	<property name="SoundPickup" value="bullets_grab"/>
	<property name="SoundPlace" value="bullets_place"/>
	<effect_group name="ammoIncendiaryBullet9mm" tiered="false">
		<passive_effect name="EntityDamage" operation="base_set" value="45" tags="perkGunslinger,9mmGun"/>
		<passive_effect name="BlockDamage" operation="base_set" value="2" tags="perkGunslinger"/>
		<!-- Fire effect on hit -->
		<triggered_effect trigger="onSelfAttackedOther" action="ModifyCVar" target="other" cvar="$buffBurningElementDuration" operation="set" value="10"/>
		<triggered_effect trigger="onSelfAttackedOther" action="AddBuff" target="other" buff="buffBurningElement">
			<requirement name="CVarCompare" target="other" cvar="_underwater" operation="LT" value="0.3"/>
		</triggered_effect>
	</effect_group>
</item>
</append>

