<?xml version="1.0" encoding="UTF-8"?>
<configs>

<!-- *** URANIUM AMMUNITION *** -->

<!-- 9mm Uranium Bullet -->
<append xpath="/items">
<item name="ammoUraniumBullet9mm">
	<property name="Tags" value="ammo,ammo9mm"/>
	<property name="DisplayType" value="ammoBullet"/>
	<property name="DescriptionKey" value="ammoUraniumBullet9mmDesc"/>
	<property name="CustomIcon" value="ammo9mmBulletBall"/>
	<property name="CustomIconTint" value="66FF66"/>
	<property name="HoldType" value="45"/>
	<property name="Meshfile" value="@:Other/Items/Misc/sackPrefab.prefab"/>
	<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
	<property name="Material" value="Mbrass"/>
	<property name="MeltTimePerUnit" value=".4"/>
	<property name="Stacknumber" value="150"/>
	<property name="EconomicValue" value="50"/>
	<property name="TraderStageTemplate" value="ammoTier3"/>
	<property name="Group" value="Ammo/Weapons,Ammo,Ranged Weapons"/>
	<property name="SoundPickup" value="bullets_grab"/>
	<property name="SoundPlace" value="bullets_place"/>
	<effect_group name="ammoUraniumBullet9mm" tiered="false">
		<passive_effect name="EntityDamage" operation="base_set" value="85" tags="perkGunslinger,9mmGun"/>
		<passive_effect name="BlockDamage" operation="base_set" value="4" tags="perkGunslinger"/>
		<passive_effect name="TargetArmor" operation="perc_add" value="-.85" tags="perkGunslinger"/>
		<display_value name="dTargetArmor" value="-.85"/>
		<passive_effect name="EntityPenetrationCount" operation="base_set" value="3" tags="perkGunslinger"/>
		<display_value name="dTargetPenetration" value="3"/>
		<passive_effect name="DamageModifier" operation="perc_add" value="-.8" tags="earth"/>
		<passive_effect name="DamageModifier" operation="perc_add" value="2" tags="wood"/>
		<!-- Uranium radiation poisoning on hit -->
		<triggered_effect trigger="onSelfAttackedOther" action="AddBuff" target="other" buff="buffUraniumRadiation"/>
	</effect_group>
</item>
</append>

