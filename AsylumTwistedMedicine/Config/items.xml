<config>
  <append xpath="/items">
  
<item name="curebiotics">
	<property name="Tags" value="medical,medicalSkill,fitness,asylum"/>
	<property name="HoldType" value="16"/>
	<property name="Meshfile" value="@:Other/Items/Health/painkillersPrefab.prefab"/>
	<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
	<property name="Material" value="Morganic"/>
   <property name="EconomicValue" value="15"/>
    <property name="EconomicBundleSize" value="1"/>
    <property name="SellableToTrader" value="false"/>
    <property name="CraftingIngredientTime" value="8"/>
	<property name="UnlockedBy" value="craftingMedical"/>
	<property name="Stacknumber" value="8"/>
	<property name="DisplayType" value="cureInfection"/>
	<property name="Group" value="Science,CFMedical,Medical,TCMedical,AsylumMedicine"/>
	<property name="SoundPickup" value="drug_antibiotics_grab"/>
	<property name="SoundPlace" value="drug_antibiotics_place"/>
	<property name="CustomIcon" value="curbiotics"/>
	<property class="Action0">
		<property name="Class" value="Eat"/>
		<property name="Delay" value="1.5"/>
		<property name="Sound_start" value="player_painkillers"/>
	</property>
	<effect_group tiered="false">
		<display_value name="dCureInfection" value=".20"/>
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="$buffInfectionAddCurePerc" operation="add" value="20"/>
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffInfectionAddCure"/>
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffAsylumCorruption"/>
	</effect_group>
</item>

<item name="theswiftshot">
	<property name="Extends" value="drugPainkillers"/>
	<property name="DisplayType" value="drugSteroids"/>
	<property name="Tags" value="medical,medicalSkill,fitness,asylum"/>
	<property name="EconomicValue" value="350"/>
	<property name="EconomicBundleSize" value="1"/>
	<property name="UnlockedBy" value="craftingMedical"/>
	<property name="CraftingIngredientTime" value="8"/>
	<property name="Group" value="Science,Medical,TCMedical,AsylumMedicine"/>
    <property name="DescriptionKey" value="theswiftshotDesc"/>
	<property name="SoundPickup" value="steroids_grab"/>
	<property name="SoundPlace" value="steroids_place"/>
	<property name="CustomIcon" value="swiftshot"/>
	<effect_group tiered="false" name="Asylum Adrenaline Tier">
		<display_value name="dCarryCapacity" value="80"/>
		<display_value name="dRunSpeed" value=".3"/>
		<display_value name="$waterAmountAdd" value="-60"/>
		<display_value name="dDuration" value="900"/>
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="$buffDrugSteroidsDuration" operation="add" value="903"/>
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="$buffDrugSteroidsDuration" operation="set" value="2709">
			<requirement name="CVarCompare" cvar="$buffDrugSteroidsDuration" operation="GT" value="2709"/></triggered_effect>
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffDrugSteroids"/>
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffAsylumAdrenaline"/>
	</effect_group>
</item>

<item name="bleednomore">
			<property name="Extends" value="medicalFirstAidBandage"/>
			<property name="Tags" value="medical,medicalSkill,stopsBleeding,asylum"/>
	        <property name="HoldType" value="16"/>
	        <property name="Meshfile" value="@:Other/Items/Health/painkillersPrefab.prefab"/>
	        <property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
