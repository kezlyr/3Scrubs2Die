<!-- CRAFTING TOOLS -->

    <append xpath="//lootgroup[@name='workstationToolsScaled']">
        <item name="toolForgeGrinder" loot_prob_template="ProbT1Cap"/>
        <item name="toolForgeTongs" loot_prob_template="ProbT1Cap"/>
	</append>

<!-- CRAFTING TOOLS MAGAZINE -->

    <append xpath="//lootgroup[contains (@name, 'skillMagazines')
						or (contains (@name, 'groupWorkingStiffsBooks'))
						or (contains (@name, 'groupAirDrop03'))]">

        <item name="machineToolsSkillMagazine"/>
	</append>

    <append xpath="//lootgroup[contains (@name, 'groupQuestRewardSkillMagazinesAll01')
						or (contains (@name, 'groupChallengeRewardSkillMagazinesAll01'))
						or (contains (@name, 'groupAirDrop03'))]">

        <item name="machineToolsSkillMagazine" count="2"/>
	</append>

    <append xpath="//lootgroup[contains (@name, 'groupWorkingStiffs01')
						or (contains (@name, 'groupWorkbenchLoot04'))
                        or (contains (@name, 'groupChemistryStationLoot03'))
                        or (contains (@name, 'groupDewCollectorLoot01'))]">

        <item name="machineToolsSkillMagazine" loot_prob_template="high"/>
	</append>

    <append xpath="//lootgroup[contains (@name, 'groupForge')
						or (contains (@name, 'groupCementMixerLoot'))]">

        <item name="machineToolsSkillMagazine" loot_prob_template="high" force_prob="true"/>
	</append>

    <append xpath="//lootgroup[contains (@name, 'groupDewCollectorLoot02')
						or (contains (@name, 'groupConstructionCrate02'))]">

        <item name="machineToolsSkillMagazine" loot_prob_template="med"/>
	</append>

    <append xpath="//lootgroup[contains (@name, 'groupLootChestBonusItems')]">
        <item name="machineToolsSkillMagazine" count="1" loot_prob_template="med"/>
	</append>

