<!-- SCRAP TOOLS -->

    <append xpath="//lootgroup[contains (@name, 'groupToolsT0')
						or (contains (@name, 'prefabQuestLoot'))
						or (contains (@name, 'groupRottenChest03'))
                        or (contains (@name, 'groupOldWestChest02'))]">

        <item name="meleeToolPickT0ScrapPickaxe"/>
        <item name="meleeToolShovelT0ScrapShovel"/>
        <item name="meleeToolAxeT0ScrapAxe"/>
	</append>

<!-- TITANIUM TOOLS -->

    <append xpath="//lootgroup[contains (@name, 'prefabQuestLoot')
						or (contains (@name, 'groupToolsT2'))
						or (contains (@name, 'groupQuestToolsT2'))
                        or (contains (@name, 'groupInfestedT5MeleeRanged'))
                        or (contains (@name, 'twitch_tier2tool'))]">

        <item name="meleeToolAxeT2TitaniumAxe"/>
        <item name="meleeToolPickT2TitaniumPickaxe"/>
        <item name="meleeToolShovelT2TitaniumShovel"/>
	</append>

    <append xpath="//lootgroup[@name='toolParts']">
        <item name="meleeToolAllTitaniumParts" count="1,3"/>
    </append>

    <append xpath="//lootgroup[@name='groupQuestMeleeCrafting']">
        <item name="questRewardTitaniumToolsCraftingBundle"/>
    </append>

<!-- JACKHAMMER - REMOVED -->

    <!-- Jackhammer loot entries removed - Auger restored to vanilla functionality
    <append xpath="//lootgroup[contains (@name, 'groupToolsT3')
						or (contains (@name, 'groupQuestToolsT3'))
						or (contains (@name, 'groupInfestedT5MeleeRanged'))
                        or (contains (@name, 'twitch_toolauger'))
                        or (contains (@name, 'twitch_tier3tool'))]">

